{
	"name": "fuiz-enhanced-production",
	"version": "2.0.0",
	"private": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
		"dev:frontend": "vite dev",
		"dev:backend": "nodemon server/index.js",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node server/index.js",
		"deploy": "npm run build && npm start",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide"
	},
	"devDependencies": {
		"@inlang/paraglide-sveltekit": "^0.15.5",
		"@sveltejs/adapter-node": "^2.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node": "^20.0.0",
		"@types/object-hash": "^3.0.6",
		"@types/qrcode": "^1.5.5",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"concurrently": "^8.0.0",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.0.0",
		"mdsvex": "^0.12.3",
		"nodemon": "^3.0.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^4.0.0",
		"svelte-check": "^3.0.0",
		"svelte-dnd-action": "^0.9.57",
		"svelte-preprocess-import-assets": "^1.1.0",
		"super-sitemap": "^1.0.3",
		"tslib": "^2.8.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0"
	},
	"dependencies": {
		"@auth/d1-adapter": "^1.7.4",
		"@auth/sveltekit": "^1.0.0",
		"@fontsource/atkinson-hyperlegible": "^5.1.1",
		"@fontsource/poppins": "^5.1.1",
		"@inlang/paraglide-js": "1.11.8",
		"@ltd/j-toml": "^1.38.0",
		"@oddbird/popover-polyfill": "^0.5.2",
		"@prisma/client": "^5.0.0",
		"@svelte-put/inline-svg": "^4.0.1",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.18.0",
		"helmet": "^7.0.0",
		"jszip": "^3.10.1",
		"jsonwebtoken": "^9.0.0",
		"object-hash": "^3.0.0",
		"oslo": "^1.2.1",
		"papaparse": "^5.4.1",
		"prisma": "^5.0.0",
		"qrcode": "^1.5.4",
		"query-string": "^9.1.1",
		"socket.io": "^4.7.0",
		"socket.io-client": "^4.7.0",
		"svelte-confetti": "^2.3.1",
		"svelte-headlessui": "^0.0.45",
		"tippy.js": "^6.3.7",
		"uuid": "^9.0.0",
		"winston": "^3.10.0",
		"zod": "^3.22.0"
	},
	"type": "module"
} 